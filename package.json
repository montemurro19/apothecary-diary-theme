{
  "name": "apothecary-diary-theme",
  "displayName": "Apothecary Diary Theme",
  "description": "Themes for VSCode inspired by the aesthetics of apothecary diaries.",
  "version": "0.1.0",
  "icon": "images/icon.png",
  "publisher": "apothecary-diary-theme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/montemurro19/apothecary-diary-theme.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "keywords": ["theme", "vscode", "apothecary", "diary", "jinshi", "maomao"],
  "categories": [
    "Themes"
  ],
  "files": [
    "themes/**",
    "images/**",
    "README.md",
    "LICENSE"
  ],
  "galleryBanner": {
    "color": "#F8F7F3",
    "theme": "light"
  },
  "homepage": "https://github.com/montemurro19/apothecary-diary-theme",
  "bugs": {
    "url": "https://github.com/montemurro19/apothecary-diary-theme/issues"
  },
  "scripts": {
    "package": "npx vsce package",
    "install:theme": "npx vsce package && code --install-extension apothecary-diary-theme-$(node -p \"require('./package.json').version\").vsix",
    "publish": "npx vsce publish"
  },
  "contributes": {
    "themes": [
      {
        "label": "Maomao Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/maomao.json"
      },
      {
        "label": "Maomao Light Theme",
        "uiTheme": "vs",
        "path": "./themes/ligth/maomao.json"
      },
      {
        "label": "Jinshi Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/jinshi.json"
      },
      {
        "label": "Jinshi Light Theme",
        "uiTheme": "vs",
        "path": "./themes/ligth/jinshi.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  }
}